{
  "expo": {
    "name": "Agenda Planner",
    "slug": "agenda-planner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-tomato.png",
      "resizeMode": "contain",
      "backgroundColor": "#d77e70"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-tomato.png",
        "backgroundColor": "#000000"
      },
      "package": "com.camigrilletti.agendaplanner"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Poppins-Regular.ttf"
          ]
        }
      ]
    ],
    "owner": "camigrilletti",
    "extra": {
      "eas": {
        "projectId": "c436b81f-7678-43b5-a9d5-ad59159121df"
      }
    }
  }
}
