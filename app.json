{
  "expo": {
    "name": "agenda-planner",
    "slug": "agenda-planner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-tomato.png",
      "resizeMode": "contain",
      "backgroundColor": "#d77e70"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-tomato.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Poppins-Regular.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "9442d295-ef4e-4e44-80a9-90816eb7fa6c"
      }
    },
    "owner": "camigrilletti"
  }
}
